INPUT

T_GaimObj
	$var = gaim_perl_ref_object($arg)

OUTPUT

T_GaimObj
	$arg = gaim_perl_bless_object($var, \"$type\");

T_GCHAR_OWN
	/* used when we can directly own the returned string. */
	/* we have to copy in the case when perl's malloc != gtk's malloc,
	 * so best copy all the time. */
	sv_setpv ((SV*)$arg, $var);
	SvUTF8_on ($arg);
	g_free ($var);

